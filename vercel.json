{
  "buildCommand": "yarn build:client",
  "outputDirectory": "packages/client/dist",
  "installCommand": "yarn install",
  "framework": "vite",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
